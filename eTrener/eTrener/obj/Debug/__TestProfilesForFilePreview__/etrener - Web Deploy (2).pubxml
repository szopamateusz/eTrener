    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="eTrener" Order="1" Enabled="True">
          <Destination Path="Data Source=tcp:etrenerdbserver.database.windows.net,1433;Initial Catalog=etrenerdb;Persist Security Info=False;User ID=mateusz;Password=zaq1@WSX;Connect Timeout=30;Encrypt=True;TrustServerCertificate=False" Name="" />

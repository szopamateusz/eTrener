
<table id="products" class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Calories</th>
            <th>Fat</th>
            <th>Carbs</th>
            <th>Protein</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var position in Model)
        {
            <tr>      
                <td>@position.Name</td>
                <td>@position.Calories</td>
                <td>@position.Fat</td>
                <td>@position.Carbs</td>
                <td>@position.Protein</td>
                <td>@Html.ActionLink("Delete","DeleteProduct","Diet",new {id=@position.ProductId },null)</td>
            </tr>
        }

    </tbody>
    <tfoot>
        <tr>
            <th>Name</th>
            <th>Calories</th>
            <th>Fat</th>
            <th>Carbs</th>
            <th>Protein</th>
            <th>Delete</th>
        </tr>
    </tfoot>
</table>




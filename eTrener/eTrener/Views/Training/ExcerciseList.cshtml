@model PagedList.IPagedList<eTrener.Models.Excercise>
@using PagedList.Mvc;
@{
    ViewBag.Title = "List of excercises";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="box">
    <div class="box-header">
        <h3 class="box-title">Excercise List</h3>
    </div>
    <div class="box-body">
        @Html.Partial("_Excercise")
    </div>
    <div class="box-body">
        Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) of @Model.PageCount

        @Html.PagedListPager(Model, page => Url.Action("ExcerciseList", 
            new { page}))
    </div>
</div>
<script src="~/Scripts/jquery-3.1.1.js"></script>
<script src="~/Scripts/jquery-3.1.1.min.js"></script>
<script src="~/Scripts/jquery-3.1.1.intellisense.js"></script>

